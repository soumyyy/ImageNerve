{
  "expo": {
    "name": "ImageNerve",
    "slug": "ImageNerveExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow ImageNerve to access your photo library.",
        "NSPhotoLibraryAddUsageDescription": "Allow ImageNerve to save photos to your library.",
        "UIApplicationSupportsIndirectInputEvents": true,
        "NSCameraUsageDescription": "Allow ImageNerve to use your camera to capture your face profile."
      },
      "bundleIdentifier": "com.anonymous.ImageNerveExpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "READ_MEDIA_IMAGES",
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "CAMERA"
      ],
      "package": "com.anonymous.ImageNerveExpo"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "bfb1d2fc-244b-4400-bbc3-7d6ced8474c0"
      }
    }
  }
}
